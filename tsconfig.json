{
  "compilerOptions": {
    "esModuleInterop": true,
    "sourceMap": true,
    "module": "commonjs",
    "allowJs": true,
    "target": "ES2020",
    "outDir": ".build",
    "lib": [
      "es2015"
    ],
    "rootDir": "./",
    "baseUrl": "./src",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@handles/*": [
        "./handles/*"
      ],
    }
  },
  "exclude": [
    "node_modules/**/*",
    ".serverless/**/*",
    ".webpack/**/*",
    "_warmup/**/*",
    ".vscode/**/*"
  ]
}
